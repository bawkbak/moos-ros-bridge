ServerHost = localhost
ServerPort = 9000
Simulator  = true

Community    = moosros
MOOSTimeWarp = 1

Processconfig = ANTLER
{
  MSBetweenLaunches = 100
   
  Run = MOOSDB           @ NewConsole = false
  Run = MOOS_ROS_BRIDGE  @ NewConsole = false
  Run = uXMS              @ NewConsole = false 
  Run = pLogger          @ NewConsole = false
}


ProcessConfig = MOOS_ROS_BRIDGE
{
  AppTick    = 1
  CommsTick  = 20
}

ProcessConfig = uXMS
{
  AppTick    = 4
  CommsTick  = 4
  VAR            = boat/localization_gps_imu/odometry/header/seq,boat/localization_gps_imu/odometry/header/stamp/sec 
}

ProcessConfig = pLogger
{

  
  AsyncLOG   = true
  WildCardLOGging = true
  WildCardOmitPattern = *_STATUS
  FileTimeStamp   = true
  LOG     = boat/localization_gps_imu/odometry/header/seq           @ 0
  LOG     = boat/localization_gps_imu/odometry/header/stamp/sec     @ 0
  LOG     = boat/localization_gps_imu/odometry/header/stamp/nsec    @ 0
  LOG     = boat/localization_gps_imu/odometry/header/frame_id      @ 0
  LOG     = boat/localization_gps_imu/odometry/child_frame_id       @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/position/x      @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/position/y      @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/position/z      @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/orientation/x   @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/orientation/y   @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/orientation/z   @ 0
  LOG     = boat/localization_gps_imu/odometry/pose/orientation/w   @ 0
  LOG     = boat/localization_gps_imu/odometry/twist/linear/x       @ 0
  LOG     = boat/localization_gps_imu/odometry/twist/linear/y       @ 0
  LOG     = boat/localization_gps_imu/odometry/twist/linear/z       @ 0
  LOG     = boat/localization_gps_imu/odometry/twist/angular/x      @ 0
  LOG     = boat/localization_gps_imu/odometry/twist/angular/y      @ 0
  LOG     = boat/localization_gps_imu/odometry/twist/angular/z      @ 0
  

}
